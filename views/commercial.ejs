<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>First Key Finance</title>
  <!-- Include Tailwind CSS -->
  <link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.16/dist/tailwind.min.css" rel="stylesheet" />
  <!-- Include Heroicons -->
  <link href="https://cdn.jsdelivr.net/npm/@heroicons/vue@1.0.4/dist/heroicons.min.css" rel="stylesheet" />
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Lato&family=Tektur&display=swap" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
  <style>
    /* Adjust spacing and padding as needed */

    body {
      font-family: Arial, sans-serif;
    }
  </style>
</head>
<style>
  h1 {
    font-family: 'Lato', sans-serif;
    font-family: 'Tektur', cursive;
  }
</style>

<body class="bg-white">
  <nav class="bg-gray-100">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="flex items-center justify-between h-9">
        <div class="flex-shrink-0">
          <!-- Logo or Branding -->
          <a href="/" class="text-gray-900">Personal</a>
          <a href="/commercial" class="text-gray-900 underline">Commercial</a>
        </div>
        <div class="hidden md:block">
          <!-- Desktop Navbar links -->
          <ul class="ml-4 flex space-x-8 space-y-auto">

            <li>
              <a href="#about" class="text-gray-900 hover:text-white">About Us</a>
            </li>

            <li>
              <a href="#faqs" class="text-gray-900 hover:text-white">FAQs</a>
            </li>
            <li>
              <a href="#contact" class="text-gray-900 hover:text-white">Contact Us</a>
            </li>


            </li>
          </ul>
        </div>
        <!-- Mobile menu button -->
        <div class="md:hidden">
          <button id="mobile-menu-button" class="text-white focus:outline-none">
            <svg class="h-6 w-6" fill="none" stroke="currentColor" viewBox="0 0 24 24"
              xmlns="http://www.w3.org/2000/svg">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16"></path>
            </svg>
          </button>
        </div>
      </div>
      <!-- Mobile Navbar links (hidden by default) -->
      <div class="hidden" id="mobile-menu">
        <ul class="space-y-2">
          <li>
            <a href="/" class="text-gray-300 hover:text-white">Home</a>
          </li>
          <li>
            <a href="#about" class="text-gray-300 hover:text-white">About</a>
          </li>
          <li>
            <a href="#services" class="text-gray-300 hover:text-white">Services</a>
          </li>
          <li>
            <a href="#pricing" class="text-gray-300 hover:text-white">Pricing</a>
          </li>
          <li>
            <a href="#faqs" class="text-gray-300 hover:text-white">FAQs</a>
          </li>
          <li>
            <a href="#contact" class="text-gray-300 hover:text-white">Contact</a>
          </li>
          <li>
            <a href="/signup">
              <button class="bg-yellow-600 hover:bg-yellow-600 text-white font-semibold py-2 px-4 rounded-sm">
                Sign up
              </button>
            </a>
          </li>
          <li>
            <a href="/login">
              <button class="bg-blue-600 hover:bg-blue-600 text-white font-semibold py-2 px-4 rounded-sm">
                Login
              </button>
            </a>
          </li>

        </ul>
      </div>
    </div>
  </nav>
  <nav class="bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="flex items-center justify-between h-16">
        <div class="flex-shrink-0">
          <!-- Logo or Branding -->
          <a href="/" class="text-gray-900 text-3xl">First Key Finance</a>
        </div>
      </div>

    </div>
  </nav>
  <h2 class="text-center text-4xl mt-40 font-bold">COMING SOON!!!</h2>

  <!-- Chat Icon -->



</body>


<script>
  const mobileMenuButton = document.getElementById("mobile-menu-button");
  const mobileMenu = document.getElementById("mobile-menu");

  mobileMenuButton.addEventListener("click", () => {
    mobileMenu.classList.toggle("hidden");
  });
</script>

<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

</html>